{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "REACT_APP_MOCK_FORM=true yarn start",
      "name": "Start dev server with mock form data",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}